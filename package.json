{
  "name": "file-move-refactor",
  "version": "1.0.0",
  "description": "Script to move files and update imports automatically",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "move": "node ./src/index.js",
    "test-move": "node ./src/index.js --dry-run",
    "build": "tsc",
    "postbuild": "node scripts/fix-extensions.js",
    "start": "npm run build && node --max-old-space-size=16000 ./dist/index.js",
    "watch": "tsc --watch --project tsconfig.json",
    "startv": "npm run build && node --max-old-space-size=16000 ./dist/index.js --verbose"
  },
  "dependencies": {
    "@babel/parser": "^7.27.5",
    "@babel/traverse": "^7.27.4",
    "fast-glob": "^3.3.3",
    "glob": "^10.3.10"
  },
  "bin": {
    "move-file": "./src/index.js"
  },
  "keywords": [
    "refactor",
    "imports",
    "file-move"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/babel__traverse": "^7.20.7",
    "@types/node": "^22.15.30",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.28.0"
  }
}
